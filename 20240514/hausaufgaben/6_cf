# Name: 6_cf
# Description: check the arguments as existing files and print out the check

if [ $# -eq 0 ]; then
	echo "expect one argument"
fi
for file in $@; do
	if [ -f $file ]; then
		echo "$file ok"
	else
		echo "$file not exist"
	fi
done


